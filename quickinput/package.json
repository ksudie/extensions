{
  "name": "samplequickinput",
  "displayName": "samplequickinput",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.43.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:samplequickinput.choosecolor",
    "onCommand:samplequickinput.chooseaction",
    "onCommand:samplequickinput.inputyourname"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "samplequickinput.choosecolor",
        "title": "Choose Color",
        "category": "SampleQuickInput"
      },
      {
        "command": "samplequickinput.chooseaction",
        "title": "Choose Action",
        "category": "SampleQuickInput"
      },
      {
        "command": "samplequickinput.inputyourname",
        "title": "Input Your Name",
        "category": "SampleQuickInput"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.43.0",
    "glob": "^7.1.5",
    "mocha": "^6.2.2",
    "typescript": "^3.6.4",
    "tslint": "^5.20.0",
    "vscode-test": "^1.2.2"
  }
}
