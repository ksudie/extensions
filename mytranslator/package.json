{
		"name": "mytranslator",
		"displayName": "mytranslator",
		"description": "",
		"version": "0.0.1",
		"engines": {
				"vscode": "^1.40.0"
		},
		"categories": [
				"Other"
		],
		"activationEvents": [
				"*"
		],
		"main": "./out/extension.js",
		"contributes": {
				"commands": [
						{
								"command": "mytranslator.translate",
								"title": "Translate",
								"category": "MyTranslator"
						}
				],
				"keybindings": [
						{
								"command": "mytranslator.translate",
								"key": "ctrl+shift+t",
								"mac": "cmd+shift+t",
								"when": "editorHasSelection"
						}
				],
				"configuration": {
						"type": "object",
						"title": "MyTranslator Configuration",
						"properties": {
								"mytranslator.subKeyTranslator": {
										"type": "string",
										"default": "********************************",
										"description": "Subscription Key for Azure Cognitive Translator API"
								},
								"mytranslator.targetLanguage": {
										"type": "string",
										"default": "ja",
										"description": "Target Language Code (en, fr, de, ja, zh-Hans,...)"
								}
						}
				}
		},
		"scripts": {
				"vscode:prepublish": "npm run compile",
				"compile": "tsc -p ./",
				"watch": "tsc -watch -p ./",
				"pretest": "npm run compile",
				"test": "node ./out/test/runTest.js"
		},
		"devDependencies": {
				"@types/glob": "^7.1.1",
				"@types/mocha": "^5.2.7",
				"@types/needle": "^2.0.4",
				"@types/node": "^12.11.7",
				"@types/vscode": "^1.40.0",
				"glob": "^7.1.5",
				"mocha": "^6.2.2",
				"tslint": "^5.20.0",
				"typescript": "^3.6.4",
				"vscode-test": "^1.2.2"
		},
		"dependencies": {
				"needle": "^2.4.0",
				"uuidv4": "^6.0.0"
		}
}
